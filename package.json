  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "node scripts/build",
    "docs": "node scripts/docs",
    "test": "tslint '{lib,test}/**/*.ts' --fix && jest test/*.spec.ts --verbose -i",
    "precommit": "lint-staged",
    "format": "prettier '{lib,test}/**/*.[tj]s' --write",
    "version": "mdchangelog --remote stardog-union/stardog.js --no-prologue --order-milestones semver --order-issues closed_at --overwrite --no-orphan-issues && npm run docs && git add README.md CHANGELOG.md",
    "prepublishOnly": "node scripts/triggerAnnoyingAlert.js && npm run build"
  },
  "lint-staged": {
    "linters": {
      "{lib,test}/**/*.[tj]s": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "stardog-version": ">=5.0.0"
